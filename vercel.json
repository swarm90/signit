{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/signit.mobileconfig",
      "dest": "/public/signit.mobileconfig",
      "headers": {
        "Content-Type": "application/x-apple-aspen-config",
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|webp|woff|woff2|ttf|eot|json))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
